        <value>var userid = current.variables.user.x_dmr_okta_group_m_okta_user_id;&#13;
var groupid = current.variables.okta_group.okta_id;&#13;
var groupname = current.variables.okta_group.okta_group_name;&#13;
gs.info("User equals: "+ userid);&#13;
gs.info("Group is: "+ groupid);&#13;
gs.info("Group is: "+ groupname);&#13;
&#13;
var okta_url = gs.getProperty("x_dmr_okta_group_m.OktaInstance");&#13;
var okta_api = gs.getProperty("x_dmr_okta_group_m.OktaAPIKey");&#13;
&#13;
gs.info("url is: "+ okta_url);&#13;
gs.info("api key is: "+ okta_api);&#13;
&#13;
&#13;
var getrestMessage = new sn_ws.RESTMessageV2();&#13;
var url = okta_url + "/api/v1/groups/"+groupid+"/users/"+userid ;&#13;
gs.info("url call is: "+ url);&#13;
getrestMessage.setHttpMethod("delete");&#13;
getrestMessage.setRequestHeader("Accept","application/json");&#13;
getrestMessage.setRequestHeader("Content-Type","application/json");&#13;
getrestMessage.setRequestHeader("Authorization",okta_api);&#13;
getrestMessage.setEndpoint(url);&#13;
//getrestMessage.setHttpTimeout(10000);&#13;
var responseBody = getrestMessage.execute();&#13;
responseBody.waitForResponse(60);&#13;
//gs.info(responseBody);&#13;
&#13;
//remove user from group.&#13;
//Get user sys_id&#13;
&#13;
var rec = new GlideRecord('sys_user');&#13;
rec.addQuery('x_dmr_okta_group_m_okta_user_id', userid);&#13;
rec.query();&#13;
while(rec.next()){&#13;
	var o_user = rec.sys_id;&#13;
	gs.info("user is: " + o_user);&#13;
}&#13;
var rec2 = new GlideRecord('x_dmr_okta_group_m_okta_group_management_table');&#13;
rec2.addQuery('okta_group_name', groupname);&#13;
rec2.query();&#13;
while(rec2.next()){&#13;
	var group_sysid = rec2.sys_id;&#13;
	gs.info("Sys ID is: " + group_sysid);&#13;
}&#13;
&#13;
var gr = new GlideRecord('x_dmr_okta_group_m_okta_user_group_members');&#13;
gr.addQuery('okta_group',group_sysid);&#13;
gr.addQuery('user',o_user);&#13;
gr.query();&#13;
while (gr.next()) {&#13;
	//Delete each record in the query result set&#13;
	gs.info("deleting record of sys_id = " + gr.sys_id);&#13;
	gr.deleteRecord();&#13;
}&#13;
&#13;
&#13;
&#13;
&#13;
