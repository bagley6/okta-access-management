        <sys_class_name>sys_variable_value</sys_class_name>
        <sys_created_by>doug.bagley</sys_created_by>
        <sys_created_on>2016-09-21 23:09:21</sys_created_on>
        <sys_id>0dc8084513426e00e0e253c32244b02e</sys_id>
        <sys_mod_count>2</sys_mod_count>
        <sys_updated_by>doug.bagley</sys_updated_by>
        <sys_updated_on>2016-09-21 23:26:43</sys_updated_on>
        <value>var okta_url = gs.getProperty("x_dmr_okta_group_m.OktaInstance");&#13;
var okta_api = gs.getProperty("x_dmr_okta_group_m.OktaAPIKey");&#13;
&#13;
var getrestMessage = new sn_ws.RESTMessageV2(); &#13;
var oktaUser = current.request.requested_for.email;&#13;
var url = okta_url + "/api/v1/users/"+oktaUser; &#13;
&#13;
getrestMessage.setHttpMethod("get"); &#13;
getrestMessage.setRequestHeader("Accept","application/json"); &#13;
getrestMessage.setRequestHeader("Content-Type","application/json"); &#13;
getrestMessage.setRequestHeader("Authorization",okta_api);&#13;
getrestMessage.setEndpoint(url); &#13;
getrestMessage.setHttpTimeout(10000); &#13;
var response = getrestMessage.execute(); &#13;
response.waitForResponse(60);&#13;
&#13;
var jsonString = response.getBody();  &#13;
var parsed = global.JSON.parse(jsonString);&#13;
var id=parsed.id;&#13;
//gs.log("UserID" + parsed.id);&#13;
//gs.log("Login" + parsed.profile.login);&#13;
&#13;
&#13;
var postrestMessage = new sn_ws.RESTMessageV2(); &#13;
var posturl = okta_url + "/api/v1/users/"+id+"/roles"; &#13;
var reqbody='{"type": "READ_ONLY_ADMIN"}';&#13;
postrestMessage.setHttpMethod("post"); &#13;
postrestMessage.setRequestHeader("Accept","application/json"); &#13;
postrestMessage.setRequestHeader("Content-Type","application/json"); postrestMessage.setRequestHeader("Authorization",okta_api);&#13;
postrestMessage.setEndpoint(posturl);&#13;
postrestMessage.setRequestBody(reqbody);&#13;
postrestMessage.setHttpTimeout(10000); &#13;
var postresponse = postrestMessage.execute(); &#13;
postresponse.waitForResponse(60);&#13;
&#13;
&#13;
// do group assignment&#13;
var groupid = gs.getProperty("x_dmr_okta_group_m.AuditorGroup");&#13;
&#13;
&#13;
var oktaUser1 = current.request.requested_for.x_dmr_okta_group_m_okta_user_id;&#13;
var getrestMessage = new sn_ws.RESTMessageV2();&#13;
var url = okta_url + "/api/v1/groups/"+groupid+"/users/"+oktaUser1 ;&#13;
gs.info("url call is: "+ url);&#13;
getrestMessage.setHttpMethod("put");&#13;
getrestMessage.setRequestHeader("Accept","application/json");&#13;
getrestMessage.setRequestHeader("Content-Type","application/json");&#13;
getrestMessage.setRequestHeader("Authorization",okta_api);&#13;
getrestMessage.setEndpoint(url);&#13;
//getrestMessage.setHttpTimeout(10000);&#13;
var responseBody = getrestMessage.execute();&#13;
responseBody.waitForResponse(60);&#13;
//gs.info(responseBody);&#13;
&#13;
// add user to group membership&#13;
// var rec = new GlideRecord('sys_user');&#13;
// rec.addQuery('x_dmr_okta_group_m_okta_user_id', oktaUser1);&#13;
// rec.query();&#13;
// while(rec.next()){&#13;
// 	var o_user = rec.sys_id;&#13;
// 	gs.info("user is: " + o_user);&#13;
// }&#13;
var o_user = current.request.requested_for.sys_id;&#13;
&#13;
var rec2 = new GlideRecord('x_dmr_okta_group_m_okta_group_management_table');&#13;
rec2.addQuery('okta_id', groupid);&#13;
rec2.query();&#13;
while(rec2.next()){&#13;
	var group_sysid = rec2.sys_id;&#13;
	gs.info("Sys ID is: " + group_sysid);&#13;
}&#13;
&#13;
var gr = new GlideRecord('x_dmr_okta_group_m_okta_user_group_members');&#13;
gr.initialize();&#13;
gr.okta_group = group_sysid;&#13;
gr.user = o_user;&#13;
gr.insert();&#13;
&#13;
</value>
